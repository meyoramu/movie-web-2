{
  "name": "cineverse",
  "version": "1.0.0",
  "description": "Production-ready movie web application with PHP backend",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "build": "npm run production",
    "test": "jest",
    "lint": "eslint resources/js --ext .js,.vue",
    "lint:fix": "eslint resources/js --ext .js,.vue --fix",
    "format": "prettier --write resources/js/**/*.{js,vue}",
    "analyze": "npm run production -- --analyze"
  },
  "keywords": [
    "movie",
    "cinema",
    "php",
    "web-application",
    "production-ready",
    "api",
    "mobile-money",
    "multi-language"
  ],
  "author": "CineVerse Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@types/jest": "^29.5.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.0",
    "eslint-plugin-vue": "^9.14.0",
    "jest": "^29.5.0",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "sass-loader": "^13.3.0",
    "tailwindcss": "^3.3.0",
    "vue-loader": "^17.1.0",
    "webpack": "^5.88.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@sweetalert2/theme-dark": "^5.0.15",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "gsap": "^3.12.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "sweetalert2": "^11.7.0",
    "swiper": "^9.4.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "vuex": "^4.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cineverse.git"
  },
  "bugs": {
